<% include ./partials/head %>
<% include ./partials/nav %>
<body>
   <% include ./partials/banner %>
   <div class="section white">
      <div class="row container section">
         <div class="row">
            <form id="mainForm" class="col s12">
               <div class="row container col s12">
                  <div class="input-field col s6 btnToLoad">
                     Enter an address
                     <input required id="enteredAddress" type="text" class="validate ">
                     <div class="input-field button">
                        <a class="waves-effect waves-light btn-large " id="getAddress"> Use my current location</a>
                     </div>
                  </div>
                  <div class="input-field col s6">
                     Category
                     <select  id="Category" name="Category">
                        <option value="" disabled selected>Pick Category</option>
                        <option value="KZFzniwnSyZfZ7v7nJ">Music</option>
                        <option value="KZFzniwnSyZfZ7v7nn">Film</option>
                        <option value="KZFzniwnSyZfZ7v7nE">Sports</option>
                        <option value="KZFzniwnSyZfZ7v7na">Arts &amp; Theatre</option>
                        <option value="KZFzniwnSyZfZ7v7n1">Other</option>
                     </select>
                  </div>
               </div>
               <div class="row container col s12">
                  <div class="input-field col s6">
                     From<input type="date" id="StartDate" class=" inline datepicker" placeholder="2017-06-17">
                  </div>
                  <div class="input-field col s6">
                     To<input placeholder="2017-06-17" type="date" id="EndDate" class=" inline datepicker">
                  </div>
               </div>
               <div class="row container col s12">
                  <div class="input-field col s6">
                     <div class="range-field">Radius 
                        <input class="col" type="range" value=10 id="radius" step="10" min="10" max="50" />
                     </div>
                  </div>
                  <div  class=" col s6 input-field button submit">
                     <button href="#mainmap" class="btn-large waves-effect waves-light try" type="submit" name="action">Submit  
                     <i class="material-icons">send</i>
                     </button>
                   </div>
               </div>    
            </form>
         </div>
      </div>
   </div>
   <div class="parallax-container separator startmap">
      <div class="parallax"><img src="images/CvFeXKY.jpg"></div>
   </div>
   <div class="col  scrollspy s12" id="mainmap">
      <div id="googleMap" style="width:100%;height:400px;"></div>
   </div>
   <div class="parallax-container separator">
      <div class="parallax"><img src="images/bridge.jpg"></div>
   <div class="col s12" id="total"></div>

   </div>
   <div id="result-title" class="section white"></div>
   <div class=" cardcont white">
   <dt class="col s12" id="results"></dt>
   </div>
   <% include ./partials/scripts %>
   <% include ./partials/footer %>
   <script src="javascripts/scripts.js"></script>
   <script async defer
    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDGAvilTckkKG78sYL-rparmVQ6Rw85Png&callback=mapInitialize&libraries=places">
    </script>
</body>
</html>